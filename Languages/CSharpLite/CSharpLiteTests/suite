class TBase {
  public TBase() { }
  public virtual int foo(int i) { return 1; }
  public virtual int foo(char c) { return 3; }
}

class Test : TBase {

  public Test() { }

  public override int foo(int i) { return 2; }

  public static int Main() {
    Test t = new Test();
    int a = t.foo(1) - 2;
    int b = t.foo('1') - 3;
    return a*a+b*b;
  }
}
