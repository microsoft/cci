`
class X {
  public static void Main() {
    int[][][][][] x = null;

    x = new int[5][][][][];
    for (int a = 0; a < 5; a++) {
      x[a] = new int[9][][][];
      for (int b = 0; b < 9; b++) {
        x[a][b] = new int[4][][];
        for (int c = 0; c < 4; c++) {
          x[a][b][c] = new int[7][];
          for (int d = 0; d < 7; d++) {
            x[a][b][c][d] = new int[12];
          }
        }
      }
    }
  }
}
