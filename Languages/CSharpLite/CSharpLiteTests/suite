class myClass {
  public int x;
  public int y;
}

class A {
  public static int Main() {
    myClass[][] arr = new myClass[2][];
    for (int x = 0; x < arr.Length; x++)
      arr[x] = new myClass[3];

    for (int a=0; a < 2; a++)
      for (int b=0; b < 3; b++)
        arr[a][b] = new myClass();

    myClass mc = new myClass();
    mc.x = 4;
    mc.y = 5;

    arr[1][1] = mc;

    if (arr[1][1].x == 4 && arr[1][1].y == 5)
      return 0;

    return 1;
  }
}
