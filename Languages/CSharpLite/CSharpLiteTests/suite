class Test {
  public static int Echo(int i) {
    System.Console.WriteLine(i);
    return 0;
  }

  public static int Main() {
    decimal[] one = new decimal[1];
    one[0] = 1m;
    decimal two = 2m;
    if (one[Echo(0)]++ == two) return 1;
    if (one[Echo(1)]-- != two) return 1;
    return 0;
  }
}
