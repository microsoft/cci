    System.Console.WriteLine(s);
    return result;
  }

  public static int Main() {
    if (BadFunc(true, "one") && BadFunc(false, "two")) return 1;
    if (BadFunc(false, "three") && BadFunc(true, "four")) return 1;
    return 0;
  }
}
