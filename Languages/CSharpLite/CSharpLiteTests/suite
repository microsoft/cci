class Test {
  public static int ElementTaker(out int val) {
    val = 5;
    return val;
  }

  static int Main() {
    int[][] arr = new int[3][] { new int[] { 1, 2 }, new int[] { 3, 4 }, new int[] { 5, 6 } };

    if (ElementTaker(out arr[2][1]) != 5)
      return 1;

    if (arr[2][1] != 5)
      return 2;

    return 0;
  }
}
