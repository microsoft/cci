using System;

class A {
  const int myLength = 3;

  public static int Main() {
    int[][] arr = new int[myLength][] { new int[] { 1, 2 }, new int[] { 3, 4 }, new int[] { 5, 6 } };

    int result = 0;

    // verify all elements were assigned correctly
    int counter = 1;
    for (int x = 0; x < myLength; x++) {
      for (int y = 0; y < 2; y++) {
        if (arr[x][y] != counter)
          result = 1;
        counter++;
      }
    }

    return result;
  }
}
