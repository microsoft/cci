  public int y;
}

class ArrayCovariance {
  public static int Main() {
    ClassS[] cs = new ClassS[10];
    ClassT[] ct = new ClassT[10];


    for (int x = 0; x < 10; x++) {
      cs[x] = new ClassS();
      ct[x] = new ClassT();
    }

    ct = cs;

    return 0;
  }
}
