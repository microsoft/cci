    val += 5;
    return val;
  }

  static int Main() {
    int[][] arr = new int[][] { new int[] { 1, 2 }, new int[] { 3, 4 }, new int[] { 5, 6 } };

    if (ElementTaker(ref arr[2][1]) != 11)
      return 1;

    if (arr[2][1] != 11)
      return 2;

    return 0;
